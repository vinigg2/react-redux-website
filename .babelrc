{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "../../"
        ],
        "alias": {
          "@src": "./src",
          "@jsons": "./src/jsons",
          "@scss": "./src/scss",
          "@actions": "./src/actions",
          "@api": "./src/api",
          "@components": "./src/components",
          "@fonts": "./src/fonts",
          "@icons": "./src/icons",
          "@img": "./src/img",
          "@language": "./src/language",
          "@lib": "./src/lib",
          "@reducers": "./src/reducers",
          "@store": "./src/store",
          "@utils": "./src/utils",
          "@pages": "./src/pages"
        }
      }
    ]
  ]
}